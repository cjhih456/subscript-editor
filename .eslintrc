{
    "root": true,
    "plugins": ["prettier"],
    "extends": [
        "@nuxtjs",
        "@nuxtjs/eslint-config-typescript",
        "plugin:prettier/recommended"
    ],
    "ignorePatterns": ["node_modules/"],
    "rules": {
        "prettier/prettier": "error",
        "vue/no-multiple-template-root": "off"
    }
}